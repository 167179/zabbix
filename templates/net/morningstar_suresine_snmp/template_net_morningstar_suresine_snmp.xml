         <discovery_rules>
            <discovery_rule>
               <name>Battery voltage discovery</name>
               <type>DEPENDENT</type>
               <key>battery.voltage.discovery</key>
               <delay>0</delay>
               <description>Discovery for battery voltage triggers</description>
               <item_prototypes>
                  <item_prototype>
                     <name>Battery: Battery Voltage{#SINGLETON}</name>
                     <type>SNMP_AGENT</type>
                     <snmp_oid>1.3.6.1.4.1.33333.9.30.0</snmp_oid>
                     <key>battery.voltage[batteryVoltageSlow.0{#SINGLETON}]</key>
                     <history>7d</history>
                     <value_type>FLOAT</value_type>
                     <units>V</units>
                     <description>MIB: SURESINE&#13;
Description:Battery Voltage(slow)&#13;
Scaling Factor:0.0002581787109375&#13;
Units:V&#13;
Range:[0.0, 17.0]&#13;
Modbus address:0x0004</description>
                     <applications>
                        <application>
                           <name>Battery</name>
                        </application>
                     </applications>
                     <preprocessing>
                        <step>
                           <type>MULTIPLIER</type>
                           <params>2.581787109375E-4</params>
                        </step>
                     </preprocessing>
                     <trigger_prototypes>
                        <trigger_prototype>
                           <expression>{max(5m)}&lt;{#VOLTAGE.MIN.WARN}</expression>
                           <name>Battery: Low battery voltage (below {#VOLTAGE.MIN.WARN}C for 5m)</name>
                           <opdata>Current value: {ITEM.LASTVALUE1}</opdata>
                           <priority>WARNING</priority>
                           <dependencies>
                              <dependency>
                                 <name>Battery: Critically low battery voltage (below {#VOLTAGE.MIN.CRIT}C for 5m)</name>
                                 <expression>{Template Net Morningstar SureSine SNMP:battery.voltage[batteryVoltageSlow.0{#SINGLETON}].max(5m)}&lt;{#VOLTAGE.MIN.CRIT}</expression>
                              </dependency>
                           </dependencies>
                        </trigger_prototype>
                        <trigger_prototype>
                           <expression>{max(5m)}&lt;{#VOLTAGE.MIN.CRIT}</expression>
                           <name>Battery: Critically low battery voltage (below {#VOLTAGE.MIN.CRIT}C for 5m)</name>
                           <opdata>Current value: {ITEM.LASTVALUE1}</opdata>
                           <priority>HIGH</priority>
                        </trigger_prototype>
                        <trigger_prototype>
                           <expression>{min(5m)}&gt;{#VOLTAGE.MAX.WARN}</expression>
                           <name>Battery: High battery voltage (over {#VOLTAGE.MAX.WARN}C for 5m)</name>
                           <opdata>Current value: {ITEM.LASTVALUE1}</opdata>
                           <priority>WARNING</priority>
                           <dependencies>
                              <dependency>
                                 <name>Battery: Critically high battery voltage (over {#VOLTAGE.MAX.CRIT}C for 5m)</name>
                                 <expression>{Template Net Morningstar SureSine SNMP:battery.voltage[batteryVoltageSlow.0{#SINGLETON}].min(5m)}&gt;{#VOLTAGE.MAX.CRIT}</expression>
                              </dependency>
                           </dependencies>
                        </trigger_prototype>
                        <trigger_prototype>
                           <expression>{min(5m)}&gt;{#VOLTAGE.MAX.CRIT}</expression>
                           <name>Battery: Critically high battery voltage (over {#VOLTAGE.MAX.CRIT}C for 5m)</name>
                           <opdata>Current value: {ITEM.LASTVALUE1}</opdata>
                           <priority>HIGH</priority>
                        </trigger_prototype>
                     </trigger_prototypes>
                  </item_prototype>
               </item_prototypes>
               <host_prototypes/>
               <graph_prototypes>
                  <graph_prototype>
                     <name>Battery: Battery Voltage{#SINGLETON}</name>
                     <graph_items>
                        <graph_item>
                           <drawtype>GRADIENT_LINE</drawtype>
                           <color>1A7C11</color>
                           <item>
                              <host>Template Net Morningstar SureSine SNMP</host>
                              <key>battery.voltage[batteryVoltageSlow.0{#SINGLETON}]</key>
                           </item>
                        </graph_item>
                     </graph_items>
                  </graph_prototype>
               </graph_prototypes>
               <master_item>
                  <key>battery.voltage.discovery[batteryVoltageSlow.0]</key>
               </master_item>
               <preprocessing>
                  <step>
                     <type>JAVASCRIPT</type>
                     <params>var v_range = [
    [[6, 18], [12, 15, 11.5, 15.5]],
    [[20, 36], [24, 30, 23, 31]],
    [[39, 55], [48, 60, 46, 62]],
],
result = [];

for (var idx in v_range) {
    if (v_range[idx][0][0] &lt; value &amp;&amp; value &lt; v_range[idx][0][1]) {
      result = [{
            '{#VOLTAGE.MIN.WARN}': v_range[idx][1][0],
            '{#VOLTAGE.MAX.WARN}': v_range[idx][1][1],
            '{#VOLTAGE.MIN.CRIT}': v_range[idx][1][2],
            '{#VOLTAGE.MAX.CRIT}': v_range[idx][1][3],
            '{#SINGLETON}': ''
        }];
        break;
    }
}

return JSON.stringify(result);</params>
                  </step>
               </preprocessing>
            </discovery_rule>
