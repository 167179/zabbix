               <type>SNMP_AGENT</type>
               <snmp_oid>1.3.6.1.4.1.33333.9.30.0</snmp_oid>
               <key>battery.voltage[batteryVoltageSlow.0]</key>
               <history>7d</history>
               <value_type>FLOAT</value_type>
               <units>V</units>
               <description>MIB: SURESINE</description>
               <applications>
                  <application>
                     <name>Battery</name>
                  </application>
               </applications>
               <preprocessing>
                  <step>
                     <type>MULTIPLIER</type>
                     <params>2.581787109375E-4</params>
                  </step>
               </preprocessing>
               <triggers>
                  <trigger>
                     <expression>{max(5m)}&lt;{$BATTERY.VOLTAGE.MIN.WARN}</expression>
                     <name>Battery: Low battery voltage</name>
                     <opdata>Current value: {ITEM.LASTVALUE1}</opdata>
                     <priority>WARNING</priority>
                     <dependencies>
                        <dependency>
                           <name>Battery: Critically low battery voltage</name>
                           <expression>{Template Net Morningstar SureSine SNMP:battery.voltage[batteryVoltageSlow.0].max(5m)}&lt;{$BATTERY.VOLTAGE.MIN.CRIT}</expression>
                        </dependency>
                     </dependencies>
                  </trigger>
                  <trigger>
                     <expression>{max(5m)}&lt;{$BATTERY.VOLTAGE.MIN.CRIT}</expression>
                     <name>Battery: Critically low battery voltage</name>
                     <opdata>Current value: {ITEM.LASTVALUE1}</opdata>
                     <priority>HIGH</priority>
                  </trigger>
                  <trigger>
                     <expression>{min(5m)}&gt;{$BATTERY.VOLTAGE.MAX.WARN}</expression>
                     <name>Battery: High battery voltage</name>
                     <opdata>Current value: {ITEM.LASTVALUE1}</opdata>
                     <priority>WARNING</priority>
                     <dependencies>
                        <dependency>
                           <name>Battery: Critically high battery voltage</name>
                           <expression>{Template Net Morningstar SureSine SNMP:battery.voltage[batteryVoltageSlow.0].min(5m)}&gt;{$BATTERY.VOLTAGE.MAX.CRIT}</expression>
                        </dependency>
                     </dependencies>
                  </trigger>
                  <trigger>
                     <expression>{min(5m)}&gt;{$BATTERY.VOLTAGE.MAX.CRIT}</expression>
                     <name>Battery: Critically high battery voltage</name>
                     <opdata>Current value: {ITEM.LASTVALUE1}</opdata>
                     <priority>HIGH</priority>
                  </trigger>
               </triggers>
