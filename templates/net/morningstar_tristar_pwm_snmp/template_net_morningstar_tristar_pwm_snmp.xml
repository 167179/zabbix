                     <params>var v_range = [
    [[6, 18], [12, 15, 11.5, 15.5]],
    [[20, 36], [24, 30, 23, 31]],
    [[39, 55], [48, 60, 46, 62]],
],
result = [];

for (var idx in v_range) {
    if (v_range[idx][0][0] &lt; value &amp;&amp; value &lt; v_range[idx][0][1]) {
      result = [{
            '{#VOLTAGE.MIN.WARN}': v_range[idx][1][0],
            '{#VOLTAGE.MAX.WARN}': v_range[idx][1][1],
            '{#VOLTAGE.MIN.CRIT}': v_range[idx][1][2],
            '{#VOLTAGE.MAX.CRIT}': v_range[idx][1][3],
            '{#SINGLETON}': ''
        }];
        break;
    }
}

